<div id="gallery-wrap">
	<ul id="gallery">
		<% @pictures.each do |picture| %>
				<li class="picture"><%= link_to image_tag(picture.photo.slide), picture_path(picture), :class => "fancybox" %></li>
		<% end %>
	</ul>
</div>
<div id="gallery-controls">
  <%= link_to '< Terug', {}, :id => "gallery-prev" %>
	<%= link_to 'Verder >', {}, :id => "gallery-next" %>
</div>

